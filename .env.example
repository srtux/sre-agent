# LangSmith Tracing
LANGSMITH_TRACING=false
LANGSMITH_API_KEY=<your_langsmith_api_key>
LANGSMITH_PROJECT=sre-agent
# Also supports LANGCHAIN_ prefixes for compatibility
# LANGCHAIN_TRACING_V2=true
